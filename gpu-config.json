{
  "project_id": "jared-playground",
  "bucket_name": "find-gcp-gpus",
  "number_of_instances": 3,
  "instance_config": {
    "name": "demo-instance",
    "root_persistent_disk": {
      "boot": "True",
      "autoDelete": "True"
    },
    "machine_type": "a2-highgpu-1g",
    "zone": ["us-central1-c", "us-central1-a", "us-central1-b", "europe-west4-a", "europe-west4-b", "asia-southeast1-c"],
    "network_interfaces": {
      "network": "global/networks/default"
    },
    "gpu_type": "nvidia-tesla-a100",
    "number_of_gpus": 1,
    "image_project": "deeplearning-platform-release",
    "image_family": "tf2-2-4-cu110",
    "identity_and_api_access": {
      "service_account_email": "default",
      "scopes": "https://www.googleapis.com/auth/cloud-platform"
    },
    "disk_type": "pd-balanced",
    "disk_size": "50",
    "firewall_rules": [
      "http-server",
      "https-server"
    ]
  }
}